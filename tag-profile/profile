#环境变量
export MESA_GL_VERSION_OVERRIDE=4.5
#node_modules环境变量
PATH="$HOME/.local/node_modules/bin:$PATH"

#wine默认win32
export WINEARCH=win32

# xz多线程
export XZ_DEFAULTS="-T 0"

# dwmHome
export DWMHOME="${HOME}/prog/dwm/scripts"
export PATH="$DWMHOME:$PATH"

# terminal
export TERMINAL=/usr/local/bin/st

#多显卡显示
export DRI_PRIME=1
# intel
export LIBVA_DRIVER_NAME=i965 && export VDPAU_DRIVER=va_gl
# nvidia
#export LIBVA_DRIVER_NAME=vdpau && export VDPAU_DRIVER=nvidia
